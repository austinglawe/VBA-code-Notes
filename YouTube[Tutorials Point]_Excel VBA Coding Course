  Range("A1:A5").Rows.RowHeight = 20
  Range("A1:A5").Rows.RowHeight = 30
  Range("A1:A5").Rows.RowHeight = 40
  Range("A1:A5").Rows.RowHeight = 50

  Range("A1:A5").Rows.AutoFit

End Sub
