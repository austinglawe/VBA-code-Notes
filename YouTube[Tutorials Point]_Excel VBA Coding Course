  Range("A1").ColumnWidth = 25
  Range("A1").ColumnWidth = 4

  Range("A1").Columns.ColumnWidth = 15
  Range("A1").Columns.ColumnWidth = 24
  Range("A1").Columns.ColumnWidth = 5

  Range("A1").Columns.AutoFit

End Sub
