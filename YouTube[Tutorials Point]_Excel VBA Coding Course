45 | Excel VBA - Variable Usage

Sub Variables_Usage()

  Range("A1").Value = "Tutorials"
  Range("A4").Value = "Tutorials"
  Range("A9").Value = "Tutorials"
  Range("A8").Value = "Tutorials"

  var1 = "Tutorials"

  Range("A1").Value = var1
  Range("A4").Value = var1
  Range("A9").Value = var1 & var1
  Range("A8").Value = var1

End Sub
