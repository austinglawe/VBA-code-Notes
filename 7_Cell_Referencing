Sub cell_referencing()

  ActiveCell.Value = "Test1"
  ActiveCell.Value = 40

  [b5].Value = 70
  [C1:C10] = "Test2"
  'Same as [C1:C10].Value = "Test2"

  'cells([Row Number], [Column Number]
  'Give the Row Number then Column Number
  'Cell B8
  Cells(8,2).Value = "India"

  'Used very often
  Range("A1").Value = "Mumbai"
  Range("A2:A10") = "Kottayam"

End Sub()
